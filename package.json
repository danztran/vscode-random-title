{
	"name": "vscode-random-title",
	"displayName": "Random Title",
	"description": "Random title for vscode.",
  "publisher": "danztran",
	"version": "0.0.1",
	"repository": {
		"type": "git",
		"url": "git://github.com/danztran/vscode-random-title.git"
	},
	"engines": {
		"vscode": "^1.60.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "vscode-random-title.randomTitle",
				"title": "Title: Random"
			},
			{
				"command": "vscode-random-title.previousTitle",
				"title": "Title: Previous"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn esbuild-base -- --minify",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js",
		"test-compile": "tsc -p ./",
		"esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=out/main.js --external:vscode --format=cjs --platform=node",
		"esbuild": "yarn esbuild-base -- --sourcemap",
		"esbuild-watch": "yarn esbuild-base -- --sourcemap --watch"
	},
	"devDependencies": {
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@types/vscode": "^1.60.0",
		"@typescript-eslint/eslint-plugin": "^4.31.1",
		"@typescript-eslint/parser": "^4.31.1",
		"@vscode/test-electron": "^1.6.2",
		"esbuild": "^0.13.3",
		"eslint": "^7.32.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.1",
		"typescript": "^4.4.3",
		"vsce": "^1.100.1"
	}
}
